{
  "version": "3.1.1",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "filesGlob": [
    "./dist/**/*.d.ts",
    "./decl/**/*.d.ts",
    "./*.ts",
    "./example/**/*.ts"
  ],
  "files": [
    "./dist/pureupload.d.ts",
    "./example.ts",
    "./example/src/envResolver.ts",
    "./example/src/index.ts",
    "./example/src/queueRenderer.ts"
  ]
}
